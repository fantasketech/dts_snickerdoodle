&i2c0{
	status = "okay";
	clock-frequency = <100000>;

	mpu9250@68 {
		compatible = "invensense,mpu9250";
		reg = <0x68>;
		i2c-gate {
			#address-cells = <1>;
			#size-cells = <0>;
			ax8975@c {
				compatible = "ak,ak8975";
				reg = <0x0c>;
			};
		};
	};
};


